URxvt*font: -misc-fixed-medium-r-semicondensed-*-12-90-100-*-c-60-iso8859-1
! Using Xft
URxvt*font: xft:Inconsolata:size=8
!-*-{ Xft settings
Xft*dpi:       96
Xft*antialias: True
Xft*hinting:   Full
!-*-}
URxvt*foreground: white
! Appearance
URxvt*depth: 32
URxvt*background: rgba:0000/0000/0000/aaaa
URxvt*scrollBar: off

