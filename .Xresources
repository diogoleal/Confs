Xft.dpi:        96
Xft.antialias:  true
Xft.hinting:    true
Xft.rgba:       rgb
Xft.hintstyle:  hintslight
Xft.size:		16

*.background: #282828
*.foreground: #ebdbb2
! Black + DarkGrey
*.color0: #282828
*.color8: #373B41
! DarkRed + Red
*.color1: #cc241d
*.color9: #fb4934
! DarkGreen + Green
*.color2: #98971a
*.color10: #b8bb26
! DarkYellow + Yellow
*.color3: #d79921
*.color11: #fabd2f
! DarkBlue + Blue
*.color4: #458588
*.color12: #83a598
! DarkMagenta + Magenta
*.color5: #b16286
*.color13: #d3869b
! DarkCyan + Cyan
*.color6: #689d6a
*.color14: #8ec07c
! LightGrey + White
*.color7: #bdae93
*.color15: #ebdbb2

URxvt.font: xft:Hack:style=Regular:size=10, -*-Symbola-*-*-*-*-*-*-*-*-*-*-*-*
URxvt.shading: 30
URxvt.transparent: true
URxvt.scrollstyle: next
URxvt.scrollBar: false
URxvt.letterSpace: 0

URxvt.perl-ext: default,url-select
URxvt.perl-ext-common: default,clipboard
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.clipboard.copycmd:  xsel -ib
URxvt.clipboard.pastecmd: xsel -ob
URxvt.url-select.underline: true
URxvt.url-launcher: xombrero

